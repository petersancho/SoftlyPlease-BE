{
  "name": "softlyplease-compute",
  "version": "2.0.0",
  "description": "SoftlyPlease.com Rhino Compute App Server - Enterprise computational geometry platform",
  "main": "src/app.js",
  "engines": {
    "node": "18.x",
    "npm": "9.x"
  },
  "scripts": {
    "start": "node src/app.js",
    "start:dev": "NODE_ENV=development node src/app.js",
    "start:debug": "DEBUG=softlyplease-compute:* node src/app.js",
    "build:workshop": "tsc src/workshop/*.ts --outDir dist --target es2020 --module commonjs --esModuleInterop",
    "build": "npm run build:workshop",
    "postinstall": "npm run build:workshop",
    "test:health": "curl -f http://localhost:3000/health || exit 1",
    "test:ready": "curl -f http://localhost:3000/ready || exit 1",
    "test:metrics": "curl -f http://localhost:3000/metrics || exit 1",
    "lint": "echo 'Linting not configured yet'"
  },
  "dependencies": {
    "@types/node": "^20.10.0",
    "camelcase-keys": "^9.1.3",
    "compression": "^1.7.4",
    "compute-rhino3d": "^0.13.0-beta",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "http-errors": "^2.0.0",
    "md5-file": "^5.0.0",
    "memcached": "^2.2.2",
    "memjs": "^1.3.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "response-time": "^2.3.2",
    "throng": "^5.0.0",
    "typescript": "^5.3.3"
  },
  "keywords": [
    "computational-geometry",
    "grasshopper",
    "rhino-compute",
    "topology-optimization",
    "parametric-design",
    "web-api",
    "express",
    "node.js"
  ],
  "author": "SoftlyPlease Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/boi1da-proj/SoftlyPlease-Compute.git"
  }
}
