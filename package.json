{
  "name": "@mcneel/compute.rhino3d.appserver",
  "version": "0.1.12",
  "keywords": [
    "node.js",
    "expressjs",
    "compute.rhino3d",
    "rhino3d",
    "rhino3dm",
    "grasshopper3d"
  ],
  "description": "A node.js/expressjs server which communicates to a rhino3d compute geometry server to remote solve Grasshopper definitions.",
  "homepage": "https://www.rhino3d.com/compute",
  "repository": {
    "type": "git",
    "url": "https://github.com/mcneel/compute.rhino3d.appserver"
  },
  "bugs": {
    "url": "https://github.com/mcneel/compute.rhino3d.appserver/issues"
  },
  "author": {
    "name": "Robert McNeel & Associates"
  },
  "contributors": [
    {
      "name": "Luis E. Fraguada",
      "email": "luis@mcneel.com",
      "url": "https://github.com/fraguada"
    }
  ],
  "license": "MIT",
  "preferGlobal": true,
  "engines": {
    "node": "18.x"
  },
  "main": "./src/bin/www",
  "bin": {
    "compute-rhino3d-appserver": "./src/bin/www"
  },
  "scripts": {
    "start": "node ./src/bin/www",
    "lint": "eslint src/*.js --fix && eslint src/routes/*.js --fix && eslint src/bin/www --fix",
    "start-args": "node ./src/bin/www --computeUrl http://localhost:6500/",
    "sync:examples": "node scripts/sync-upstream-examples.js",
    "sync:upstream": "git fetch mcneel-appserver && git subtree pull --prefix vendor/mcneel-appserver mcneel-appserver main --squash && npm run sync:examples"
  },
  "dependencies": {
    "camelcase-keys": "^6.2.2",
    "compression": "1.7.4",
    "compute-rhino3d": "^0.13.0-beta",
    "cors": "2.8.5",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "md5-file": "5.0.0",
    "memcached": "^2.2.2",
    "memjs": "^1.2.2",
    "morgan": "~1.9.1",
    "node-cache": "^5.1.1",
    "node-fetch": "2.6.7",
    "rhino3dm": "7.15.0",
    "three": "^0.158.0",
    "throng": "4.0.0"
  },
  "devDependencies": {
    "@babel/parser": "^7.28.3",
    "@babel/traverse": "^7.28.3",
    "@playwright/test": "^1.55.0",
    "cheerio": "^1.1.2",
    "eslint": "8.7.0",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.1"
  }
}
